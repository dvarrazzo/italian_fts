suffixes

# Sostantivi maschili

flag *a:
    [^IGC]  O   >   -O,I
    [GC]    O   >   -O,HI
    I       O   >   -O,
    [^CG]   A   >   -A,I
    [CG]    A   >   -A,HI
            E   >   -E,I

# Sostantivi femminili

flag *b:
    [^GC]   A   >   -A,E
    [GC]    A   >   -A,HE

# Forme alternativa di costruzione del plurale maschile (manico -> manici)
# e femminile (freccia -> frecce)

flag *c:
    I       A   >   -IA,E
            O   >   -O,I

# Forme alternative di femminili

flag *d:
    T   O   R   E   >   -ORE,RICE
    T   O   R   E   >   -ORE,RICI

        [^CG]   A   >   -A,ESSA
        [^CG]   A   >   -A,ESSE

        [CG]    A   >   -A,HESSA
        [CG]    A   >   -A,HESSE

                E   >   SSA
                E   >   SSE

                O   >   -O,ESSA
                O   >   -O,ESSE

# Pronomi indefiniti (qualcuno, nessuno...)

flag *e:
    O       >   -O,A

# Aggettivi e sostantivi sia maschili che femminili

flag *f:
        [^IGC]  O   >   -O,I
        [GC]    O   >   -O,HI
        I       O   >   -O,
                E   >   -E,I

                O   >   -O,A
                E   >   -E,A

        [^GC]   O   >   -O,E
        [GC]    O   >   -O,HE

# Forma alternativa di plurale maschile (amico -> amici, amica, amiche)

flag *g:
                O   >   -O,I
                O   >   -O,A
        [^CG]   O   >   -O,E
        [CG]    O   >   -O,HE

# Superlativo assoluto
# Non deve andare sulle parole col flag /p, ma solo su /o e /S

flag *h:
